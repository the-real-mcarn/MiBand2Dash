{
  "name": "miband2dash",
  "version": "1.0.0",
  "description": "Simple electron dashboard for saving / storing MiBand 2 data using the MiBand-JS library. ",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run compile && electron ./build/main.js",
    "compile": "tsc && npm run pack",
    "pack": "browserify ./build/index.js -o ./build/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "//gitea.arnweb.nl/arne/miband2dash.git"
  },
  "keywords": [
    "miband",
    "node"
  ],
  "author": "Arne van Iterson",
  "license": "ISC",
  "dependencies": {
    "@mdi/font": "^5.8.55",
    "bulma": "^0.9.1",
    "chart.js": "^2.9.4",
    "miband": "^0.1.4",
    "node-web-bluetooth": "^1.2.0",
    "webbluetooth": "^2.1.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.29",
    "@types/noble": "0.0.40",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@types/chartjs": "0.0.31",
    "browserify": "^17.0.0",
    "electron": "^11.1.1",
    "electron-rebuild": "^2.3.4",
    "eslint": "^7.16.0",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.3"
  }
}
